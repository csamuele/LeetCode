from collections import Counter
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        count = Counter(s)
        for char in t:
            if char in count:
                count[char] -= 1
                if count[char] == 0:
                    del count[char]
            else:
                return False
        return True
    